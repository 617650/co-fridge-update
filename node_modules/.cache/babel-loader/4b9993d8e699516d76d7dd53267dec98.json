hangeButtonText();\r\n        });\r\n    },\r\n    computed:{\r\n      isDisabled: function(){\r\n        console.log(this.claimed)\r\n        return this.claimed;\r\n      }\r\n    },\r\n    methods: {\r\n        ListNumber(highEnd) {\r\n            var list = [];\r\n            for (var i = 1; i <= highEnd; i++) {\r\n                list.push(i);\r\n            }\r\n            return list;\r\n        },\r\n        SubmitClaim() {\r\n            axios\r\n                .post(\"/api/item/claim/\" + this.item.id, {\r\n                    quantity: this.quantity\r\n                })\r\n                .then((response) => {\r\n                    this.open = false;\r\n                    eventBus.$emit(\"claim-item-success\", {\r\n                        data: response.data,\r\n                    });\r\n                    this.quantity=0;\r\n                    // this.claimed = true;\r\n                    // this.claimButton.text = \"Claimed\";\r\n                })\r\n                .catch((error) => {\r\n                    if (error.response && error.response.status != 200) {\r\n                        alert(error.response.data.error);\r\n                    }\r\n                });\r\n        },\r\n        ChangeButtonText() {\r\n            if (this.item.status != 0) {\r\n               this.claimed=true;\r\n                this.claimButton.text = \"Out of stock\";\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.blue-color {\r\n    color: #3ca3d8;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/item"}]}